<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="core.dao.AdministratorDao" >
	<select id="selectAdministrator" resultType="core.model.Administrator">
		select * 
    	from [Administrator]
    	
    	where ID =1;
	</select>
	
</mapper>
